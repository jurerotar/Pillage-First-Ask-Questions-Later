{
  "$schema": "https://turborepo.com/schema.json",
  "globalDependencies": ["package.json", "tsconfig.json", ".editorconfig", "biome.json"],
  "tasks": {
    "build": {
      "outputs": ["build/**"]
    },
    "build:dev": {
      "outputs": ["build/**"]
    },
    "format:check": {},
    "format": {},
    "lint:check": {},
    "lint": {},
    "test": {},
    "type-check": {},
    "dev": {
      "persistent": true,
      "cache": false
    },
    "open-api:generate": {
      "outputs": ["open-api.json", "open-api.ts"]
    }
  }
}
