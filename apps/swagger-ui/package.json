{
  "$schema": "https://www.schemastore.org/package.json",
  "name": "@pillage-first/swagger-ui",
  "private": true,
  "type": "module",
  "license": "AGPL-3.0-or-later",
  "author": "Jure Rotar <hello@jurerotar.com>",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurerotar/Pillage-First-Ask-Questions-Later",
    "directory": "apps/swagger-ui"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:check": "biome lint --no-errors-on-unmatched",
    "lint": "biome lint --fix --no-errors-on-unmatched",
    "lint:check/oxlint": "oxlint --type-aware",
    "lint/oxlint": "oxlint --type-aware --fix",
    "format:check": "biome ci --linter-enabled=false",
    "format": "biome check --write --no-errors-on-unmatched",
    "type-check": "tsgo"
  },
  "dependencies": {
    "@pillage-first/api": "*",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "swagger-ui-react": "5.31.0"
  },
  "devDependencies": {
    "@biomejs/biome": "*",
    "@types/react": "19.2.11",
    "@types/react-dom": "19.2.3",
    "@types/swagger-ui-react": "5.18.0",
    "@typescript/native-preview": "*",
    "rolldown-vite": "7.3.1",
    "typescript": "*",
    "vite": "npm:rolldown-vite@7.3.1"
  },
  "scarfSettings": {
    "enabled": false
  }
}
